{
  "name": "epa-hsrp-tots-client",
  "version": "1.0.0",
  "private": true,
  "description": "Trade-off Tool for Sampling (TOTS) is JavaScript Web Mapping application to create sampling strategy for chemical and radiological contamination characterization easily accessible to field collection applications via the EPA GeoPlatform.",
  "keywords": [
    "TOTS",
    "Trade-off Tool for Sampling"
  ],
  "repository": "github:USEPA/EPA-HSRP-TOTS",
  "bugs": "https://github.com/USEPA/EPA-HSRP-TOTS/issues",
  "engines": {
    "node": "16.x"
  },
  "main": "src/index.js",
  "homepage": ".",
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:css": "stylelint './src/**/*.{js,jsx,ts,tsx,css}'"
  },
  "dependencies": {
    "@arcgis/core": "4.24.7",
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "@reach/dialog": "0.17.0",
    "@reach/window-size": "0.17.0",
    "exceljs": "4.3.0",
    "file-saver": "2.0.5",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-color": "2.19.3",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.2",
    "react-scripts": "5.0.1",
    "react-select": "5.4.0",
    "react-switch": "7.0.0",
    "react-table": "7.8.0",
    "react-tooltip": "4.2.21",
    "react-window": "1.8.7",
    "typescript": "4.7.4",
    "universal-cookie": "4.0.4",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@cypress/instrument-cra": "1.4.0",
    "@types/arcgis-js-api": "4.24.0",
    "@types/file-saver": "2.0.5",
    "@types/jest": "28.1.6",
    "@types/mocha": "9.1.1",
    "@types/node": "18.6.1",
    "@types/react": "18.0.15",
    "@types/react-color": "3.0.6",
    "@types/react-dom": "18.0.6",
    "@types/react-select": "5.0.1",
    "@types/react-table": "7.7.12",
    "@types/react-window": "1.8.5",
    "@types/uuid": "8.3.4",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "stylelint": "14.9.1",
    "stylelint-config-standard": "26.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "npm run lint:css"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie > 0",
      "not android < 5"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
